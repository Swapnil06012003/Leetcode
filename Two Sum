class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int>m;
        for(int i=0;i<nums.size();i++){
            for(int j=0;j<nums.size();j++)
            if(nums[i]+nums[j]==target && i!=j){
                m.push_back(i); m.push_back(j);
                return m;
            }
        }
        return m;
    }
};
